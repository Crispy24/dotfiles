# Automatically add keys to eliminate future passphrase-use
AddKeysToAgent yes

# For personal account use
Host github.com
     User git
     Hostname github.com
     IdentityFile=~/.ssh/id_ed25519

# For work account use
Host github-1life
     User git
     Hostname github.com
     IdentityFile=~/.ssh/id_ed25519_1life
